body, td {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 8pt;
	border-bottom: thin;
	border-bottom-color: #DDDDDD;
}

tr {
	background-color: #ffffff;
}

.initial {
	background-color: #ffffff;
	color: #000000;
}

.normal {
	background-color: #eeeeee;
}

.highlight {
	background-color: #002666;
	color: #ffffff;
	cursor: pointer;
}

input {
	border-color: #000000;
	border-style: solid;
	border-width: 1px;
	font-size: 8pt;
}

.button {
	background: #002666;
	color: #ffffff;
	font-weight: bold;
}

hr {
	height: 1px;
	color: #DDDDDD;
}

th {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 8pt;
	font-weight: bold;
	text-align: center;
	background-color: #002666;
	color: #CCD4E0;
}

th a:link {
	margin: 0;
	background-color: #4C6794;
	color: #CCD4E0;
	display: block;
	text-decoration: none;
}

th a:active {
	margin: 0;
	background-color: #4C6794;
	color: #CCD4E0;
	display: block;
	text-decoration: none;
}

th a:visited {
	margin: 0;
	background-color: #4C6794;
	color: #CCD4E0;
	display: block;
	text-decoration: none;
}

th a:hover {
	margin: 0;
	background-color: #8092B2;
	color: #ffffff;
	display: block;
	text-decoration: none;
}
