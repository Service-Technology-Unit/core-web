<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <display-name>CoreWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>/WEB-INF/applicationContext*.xml</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <listener>
    <listener-class>org.springframework.web.context.request.RequestContextListener</listener-class>
  </listener>
  <servlet>
    <description>SpringServlet</description>
    <display-name>SpringServlet</display-name>
    <servlet-name>springServlet</servlet-name>
    <servlet-class>com.sun.jersey.spi.spring.container.servlet.SpringServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <description>LookupTableServlet</description>
    <display-name>LookupTableServlet</display-name>
    <servlet-name>lookupTableServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdhs.isweb.core.servlets.GenericSpringServlet</servlet-class>
  </servlet>
  <servlet>
    <description>LookupTableJsonServlet</description>
    <display-name>LookupTableJsonServlet</display-name>
    <servlet-name>lookupTableJsonServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdhs.isweb.core.servlets.GenericSpringServlet</servlet-class>
  </servlet>
  <servlet>
    <description>LookupTableEntryServlet</description>
    <display-name>LookupTableEntyryServlet</display-name>
    <servlet-name>lookupTableEntryServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdhs.isweb.core.servlets.GenericSpringServlet</servlet-class>
  </servlet>
  <servlet>
    <description>LookupTableEntryJsonServlet</description>
    <display-name>LookupTableEntyryJsonServlet</display-name>
    <servlet-name>lookupTableEntryJsonServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdhs.isweb.core.servlets.GenericSpringServlet</servlet-class>
  </servlet>
  <servlet>
    <description>LookupTablePropertyJavascriptServlet</description>
    <display-name>LookupTablePropertyJavascriptServlet</display-name>
    <servlet-name>lookupTablePropertyJavascriptServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdhs.isweb.core.servlets.GenericSpringServlet</servlet-class>
  </servlet>
  <servlet>
    <description>LookupTableEntrySelectListServlet</description>
    <display-name>LookupTableEntrySelectListServlet</display-name>
    <servlet-name>lookupTableEntrySelectListServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdhs.isweb.core.servlets.GenericSpringServlet</servlet-class>
  </servlet>
  <servlet>
    <description>LookupTableEntrySelectListJsServlet</description>
    <display-name>LookupTableEntrySelectListJsServlet</display-name>
    <servlet-name>lookupTableEntrySelectListJsServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdhs.isweb.core.servlets.GenericSpringServlet</servlet-class>
  </servlet>
  <servlet>
    <description>NoticeTemplateServlet</description>
    <display-name>NoticeTemplateServlet</display-name>
    <servlet-name>noticeTemplateServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdhs.isweb.core.servlets.GenericSpringServlet</servlet-class>
  </servlet>
  <servlet>
    <description>SendNotificationServlet</description>
    <display-name>SendNotificationServlet</display-name>
    <servlet-name>sendNotificationServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdhs.isweb.core.servlets.GenericSpringServlet</servlet-class>
  </servlet>
  <servlet>
    <description>PasswordStrengthCalculatorServlet</description>
    <display-name>PasswordStrengthCalculatorServlet</display-name>
    <servlet-name>passwordStrengthCalculatorServlet</servlet-name>
    <servlet-class>edu.ucdavis.ucdh.stu.core.servlets.impl.PasswordStrengthCalculatorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>springServlet</servlet-name>
    <url-pattern>/data/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookupTableServlet</servlet-name>
    <url-pattern>/table/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookupTableJsonServlet</servlet-name>
    <url-pattern>/table.js</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookupTableEntryServlet</servlet-name>
    <url-pattern>/entry/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookupTableEntryJsonServlet</servlet-name>
    <url-pattern>/options/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookupTablePropertyJavascriptServlet</servlet-name>
    <url-pattern>/propertyType.js</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookupTableEntrySelectListServlet</servlet-name>
    <url-pattern>/optlist/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>lookupTableEntrySelectListJsServlet</servlet-name>
    <url-pattern>/optlistjs/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>noticeTemplateServlet</servlet-name>
    <url-pattern>/template/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>sendNotificationServlet</servlet-name>
    <url-pattern>/send</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>passwordStrengthCalculatorServlet</servlet-name>
    <url-pattern>/pwstrength</url-pattern>
  </servlet-mapping>
  <security-constraint>
    <display-name>Protected</display-name>
    <web-resource-collection>
      <web-resource-name>Protected</web-resource-name>
      <url-pattern>/entry/*</url-pattern>
      <url-pattern>/table/*</url-pattern>
      <url-pattern>/template/*</url-pattern>
      <http-method>PUT</http-method>
      <http-method>POST</http-method>
      <http-method>DELETE</http-method>
    </web-resource-collection>
    <auth-constraint>
      <description>Authenticated</description>
      <role-name>Authenticated</role-name>
    </auth-constraint>
  </security-constraint>
  <login-config>
    <auth-method>BASIC</auth-method>
    <realm-name>HS</realm-name>
  </login-config>
  <security-role>
    <description>Authenticated</description>
    <role-name>Authenticated</role-name>
  </security-role>
  <resource-ref id="ResourceRef1">
    <description>jdbc/isweb</description>
    <res-ref-name>jdbc/isweb</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
    <res-sharing-scope>Shareable</res-sharing-scope>
  </resource-ref>
</web-app>
